{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-plusplus": "off",
    "no-underscore-dangle": "off",
    "react/forbid-prop-types": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "class-methods-use-this": "off"
  }
}
